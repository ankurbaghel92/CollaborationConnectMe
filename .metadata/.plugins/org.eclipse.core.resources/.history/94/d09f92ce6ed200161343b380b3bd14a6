"use strict"
app.factory('UserServices','http','$q','$rootScope',function($http,$q,$rootScope){
	
	var BaseURL='http://localhost:8088/collaborationback'
	
		return{
		
		fetchAllUsers:function(){
			return $http.get(BaseUrl+'/allUsers')
			.then(
					function(response){
						return response.data;
					},
					null
					
			
			)
		}
	}
	
})