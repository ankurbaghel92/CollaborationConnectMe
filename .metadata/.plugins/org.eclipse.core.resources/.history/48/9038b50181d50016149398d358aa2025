"use stict"

app.factory('JobServices'['$http','$q','$rootScope',function($http,$q,$rootScope)
                          {
								var BaseURL='http://localhost:8088/collaborationback'
								return{
											
									
											fetchAllJobs : function(){
												return $http.get(BaseURL+'/alljobs').then
												(
														function(Response){
															return Response.data;
														},
														null
												)
											}
									  }
                          
                          
                          }])
