"use strict"

app.factory('EventServies','$http','$q','$scope','$rootScope',function($http,$q,$scope,$rootScope){

	var BaseURL='http://localhost:8088/collaborationback'
	
		return
		{
			fetchAllEvents:function()
			(
					return $http.get(BaseURL+'/allEvents').then
					(
							function(Response)
							{
								return Reponse.data;
							},
							null
					)
			)
			
			
		}
	
	
	
	
	
	
})












