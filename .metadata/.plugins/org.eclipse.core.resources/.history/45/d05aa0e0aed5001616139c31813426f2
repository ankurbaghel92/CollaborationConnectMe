"use strict"

app.factory('ForumServices','$http','$q','$location','$scope','$rootScope',function($http,$q,$location,$rootScope,$root){
	
	var BaseURL='http://localhost:8088/collaborationback'

	
	return{
		
		fetchAllForum : function()
		{
			return $http.get(BaseURL+'allForums')
		}
		
	}
	
	
	
	
})